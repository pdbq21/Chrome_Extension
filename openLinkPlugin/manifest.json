{
  "name": "Open Notes",
  "description": "Open notes from etsy",
  "version": "1.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "*://*/*",
    "file://*",
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_title": "Open Notes",
    "default_icon": "print_16x16.png"
  },
  "manifest_version": 2
}
