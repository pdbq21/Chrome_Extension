{
  "name": "Print this page",
  "description": "Print to the browser.",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "*://*/*",
    "file://*",
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_title": "Print this page",
    "default_icon": "print_16x16.png"
  },
  "manifest_version": 2
}
